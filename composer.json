{
    "name": "websemantics/builder-extension",
    "minimum-stability": "dev",
    "type": "streams-addon",
    "description": "Pyro 3 Addons scaffolder for maximum coding pleasure",
    "keywords": [
        "ibuild",
        "streams",
        "pyrocms",
        "laravel"
    ],
    "homepage": "http://websemantics.ca",
    "license": "MIT",
    "authors": [
        {
            "name": "Web Semantics, Inc.",
            "email": "info@websemantics.ca",
            "homepage": "http://websemantics.ca",
            "role": "Owner"
        }
    ],
    "support": {
        "email": "info@websemantics.ca"
    },
    "require":{
        "nikic/php-parser": "~2.0",
        "packaged/figlet": "*",
        "knplabs/github-api": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "Websemantics\\BuilderExtension\\": "src/",
            "PhpParser\\": "vendor/nikic/php-parser/lib/PhpParser/"
          },
        "psr-0": {
            "Github\\": "vendor/vendor/knplabs/github-api/lib/"
        },
        "files": [
            "src/Support/Functions.php"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
